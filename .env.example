# =============================================================================
# Brazilian Municipalities Analytics - Environment Configuration
# =============================================================================
# Copy this file to .env and fill in your values
# NEVER commit .env to version control!
# =============================================================================

# -----------------------------------------------------------------------------
# Google Cloud / Base dos Dados Configuration
# -----------------------------------------------------------------------------
# Your Google Cloud project ID for BigQuery billing
# Create one at: https://console.cloud.google.com/
GOOGLE_CLOUD_PROJECT=your-project-id

# Path to your GCP service account credentials JSON file
# Download from: GCP Console > IAM & Admin > Service Accounts > Keys
GOOGLE_APPLICATION_CREDENTIALS=./credentials.json

# Base dos Dados billing project (usually same as GOOGLE_CLOUD_PROJECT)
BASEDOSDADOS_BILLING_PROJECT_ID=your-project-id

# -----------------------------------------------------------------------------
# Database Configuration
# -----------------------------------------------------------------------------
# Path to the DuckDB database file
DUCKDB_DATABASE_PATH=./data/warehouse/analytics.duckdb

# -----------------------------------------------------------------------------
# Data Paths
# -----------------------------------------------------------------------------
# Directory for raw Parquet files (Bronze layer)
RAW_DATA_PATH=./data/raw

# Directory for processed data (DuckDB warehouse)
WAREHOUSE_PATH=./data/warehouse

# -----------------------------------------------------------------------------
# Application Settings
# -----------------------------------------------------------------------------
# Environment: development, staging, production
ENVIRONMENT=development

# Logging level: DEBUG, INFO, WARNING, ERROR
LOG_LEVEL=INFO

# -----------------------------------------------------------------------------
# Streamlit Configuration (Optional)
# -----------------------------------------------------------------------------
# Streamlit server port
STREAMLIT_SERVER_PORT=8501

# Enable/disable Streamlit telemetry
STREAMLIT_BROWSER_GATHER_USAGE_STATS=false
